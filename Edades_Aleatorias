/*
Esta clase genera edades de forma aleatoria con un minimo de 18 y un maximo de 27
Se implementara en una impresi√≥n en pantalla que indique los alumnos inscritos en la carrera
 */
package programa_gestionescolar;

import java.util.Random;

/**
* @author Garcia Monjaraz Jessica Stephanie
 * @author Reyna Hernandez Omara Ajany
 * @author Santiago Castro Estefanny Montsserrat
 */
public class Edades_Aleatorias {
     public static int generarEdades_Ale() {

        int max = 27;
        int min = 18;
        int y=1;
        Random rand = new Random();
        for (int e = 1; e <= 100; e++) {
        int numRandom = min + rand.nextInt((max - min) + 1);
        System.out.println( "Edad del alumno " +y+ ":  " +numRandom);
            y=y+1;
    
 }
     return 0;
 }
}
